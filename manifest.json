{
	"name": "Github Whitespace Diff",
	"version": "1.0",
	"description": "Toggle to view whitespace diffs",
	"content_security_policy": "script-src 'self' https://github.com; object-src 'self'",
	"permissions": ["webRequest", "webRequestBlocking", "storage", "*://github.com/*"],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"manifest_version": 2
}
